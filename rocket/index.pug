doctype html
html
	head
		title Rocket
		link(rel='stylesheet', href='./style.css')
		script(src='./script.js')
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1')
	body
		.stars
			- for (var i = 0; i < 60; i++)
				.star
		.moon
		.rocket-container
			.fire-container
				.glow
				- for (var i = 0; i < 5; i++)
					.fire
			.rocket
				.top
				.reflections
				.window
					.reflections
			.feet-container
				- for (var i = 0; i < 3; i++)
					.feet
			.cloud-container
				- for (var i = 0; i < 30; i++)
					.cloud
				.clouds
